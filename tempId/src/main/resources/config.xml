<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="captchaFighterService" class="Services.CaptchaFighterService"/>
    <bean id="dataExtractorService" class="Services.DataExtractorService"/>
    <bean id="htmlParserService" class="Services.HtmlParserService"/>
    <bean id="notificationService" class="Services.NotificationService"/>
    <bean id="searchService" class="MainLogic.SearchJob"/>
    <bean id="telegramBot" class="MainLogic.TelegramBot"/>

</beans>